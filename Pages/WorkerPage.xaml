<Page x:Class="ENF_Dist_Test.Pages.WorkerPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ENF_Dist_Test.Pages"
        mc:Ignorable="d"
        d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <TextBlock Text="Employee page" TextAlignment="Center" FontSize="30" Grid.Column="1" Grid.Row="0"></TextBlock>
        <TextBox Height="50" Width="200" Name="test" Grid.Column="1" Grid.Row="1"></TextBox>
        <Button Content="Back" Click="NavBack" Height="100" Width="100" VerticalAlignment="Top" Grid.Column="1" Grid.Row="2"/>
    </Grid>
</Page>